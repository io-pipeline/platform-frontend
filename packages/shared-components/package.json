    "vue",
    "vuetify",
    "components",
    "pipeline",
    "ui-library",
    "component-gallery"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "dependencies": {
    "@bufbuild/protobuf": "catalog:",
    "@connectrpc/connect": "catalog:",
    "@connectrpc/connect-web": "catalog:",
