  "repository": {
    "type": "git",
    "url": "https://github.com/ai-pipestream/platform-frontend.git",
    "directory": "packages/shared-components"
  },
  "bugs": {
    "url": "https://github.com/ai-pipestream/platform-frontend/issues"
  },
