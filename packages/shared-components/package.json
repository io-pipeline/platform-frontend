{
  "name": "@ai-pipestream/shared-components",
  "version": "1.0.7",
  "description": "Vue 3 + Vuetify 3 UI components library including ComponentGallery for Pipeline platform frontends",
  "type": "module",
  "main": "./dist/pipeline-shared-components.js",
  "module": "./dist/pipeline-shared-components.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "import": "./dist/pipeline-shared-components.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
